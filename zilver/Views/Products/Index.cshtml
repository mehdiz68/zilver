@model IEnumerable<zilver.domain.Entities.Product>

<h1>Products (Admin Only)</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Title</td>
                <td>@item.Price</td>
            </tr>
        }
    </tbody>
</table>
